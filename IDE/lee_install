function lee_install(){
	cp -arf /opt/nfshost/IDE/lee_install /etc/
	cp -arf /opt/nfshost/IDE/lee_svn /etc/
	cp -arf /opt/nfshost/IDE/lee_tools /etc/
	
	source /etc/bash.bashrc
	
	###############################################
    rm -rvf /usr/local/bin/svn_tool.sh
    rm -rvf /usr/local/bin/auto_server_reboot.sh
    
	cp -arvf /opt/nfshost/IDE/svn_tool.sh /usr/local/bin
	cp -arvf /opt/nfshost/IDE/auto_server_reboot.sh /usr/local/bin
    
}